<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="137.0"
      prefWidth="458.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="view.fxml.BinarizationProcessingWindowController">
    <children>
        <GridPane alignment="CENTER" hgap="5.0" vgap="5.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Button fx:id="manualBinarizationButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="200.0"
                        text="Binaryzacja z ustalonym progiem" GridPane.halignment="CENTER"
                        GridPane.valignment="CENTER"/>
                <Button fx:id="entropyBinarizationButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="200.0"
                        text="Selekcja entropii" GridPane.columnIndex="1" GridPane.halignment="CENTER"
                        GridPane.valignment="CENTER"/>
                <Button fx:id="percentBinarizationButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="200.0"
                        text="Procentowa selekcja czarnego" GridPane.halignment="CENTER" GridPane.rowIndex="1"
                        GridPane.valignment="CENTER"/>
                <Button fx:id="minimumBinarizationButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="200.0"
                        text="Błąd minimalny" GridPane.columnIndex="1" GridPane.halignment="CENTER"
                        GridPane.rowIndex="1" GridPane.valignment="CENTER"/>
                <Button fx:id="meanBinarizationButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="200.0"
                        text="Selekcja iteratywna średniej" GridPane.halignment="CENTER" GridPane.rowIndex="2"
                        GridPane.valignment="CENTER"/>
                <Button fx:id="fuzzyBinarizationButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="200.0"
                        text="Rozmycie błędu minimalnego" GridPane.columnIndex="1" GridPane.halignment="CENTER"
                        GridPane.rowIndex="2" GridPane.valignment="CENTER"/>
            </children>
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </VBox.margin>
        </GridPane>
        <HBox alignment="CENTER" prefHeight="68.0" prefWidth="600.0" spacing="50.0">
            <children>
                <Button fx:id="okButton" mnemonicParsing="false" text="OK"/>
                <Button fx:id="cancelButton" mnemonicParsing="false" text="Anuluj"/>
            </children>
        </HBox>
    </children>
</VBox>
